#Define mapping for menu and MVC
#If any key value associated with multiple menu items they are captured here
#Enter comma separated values within quotes
[menumap]
home			 	 = "site, preparation, shadow, cli, majorcomp"
tables_and_blocks	 = "grfttable, rtfttable, trtable, block, canopy, nxtstructure"
layout_operation 	 = "layout, canopylayout, gmstructurelayout, rtftstructurelayout,dcelayout, silayout, cilayout, gmsilayout, gmcilayout, aced,"
results				 = "layout_result, topo, electrical_analysis, nrelsam"
schedules            = "bom, wireschedule, acwireschedule"
export               = "export_acad, export_layout, export_excel"
tools_menu			 = "all_components, tools,whatif,labels,sheetSet"
settings			 =  "general_settings, about"
_displayorder	 	 = "home, tables_and_blocks, layout_operation, results, schedules, export, tools_menu, settings"

#If any class not present keep empty string.
#For GCL components, this string is same as category name.


